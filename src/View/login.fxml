<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
    <children>
        <AnchorPane layoutX="14.0" layoutY="8.0" prefHeight="385.0" prefWidth="569.0">
            <children>
                <TextField fx:id="userText" layoutX="210.0" layoutY="105.0" promptText="Enter Username" />
                <Label fx:id="userNameLabel" layoutX="259.0" layoutY="63.0" text="Username" />
                <Label fx:id="passLabel" layoutX="259.0" layoutY="176.0" text="Password" />
                <Label fx:id="locLabel" layoutX="161.0" layoutY="335.0" text="Locale" />
                <Label fx:id="locTagLabel" layoutX="344.0" layoutY="335.0" text="CurrentLocale" />
                <Button fx:id="submitButton" layoutX="259.0" layoutY="282.0" mnemonicParsing="false" onAction="#setSubmitButton" text="Submit" />
                <PasswordField fx:id="passWordPassField" layoutX="213.0" layoutY="223.0" promptText="Enter Password" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
